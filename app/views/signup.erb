<h1>Signup Page</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
<form class="form-horizontal normal" method = "POST" action = "/signup" id="signup">
  <div class="form-group">
      <input type="firstname" name="firstname" class="form-control" id="inputFirstname3" placeholder="Please enter your first name">
  </div>
  
  <div class="form-group">  
    <input type="lastname" name="lastname" class="form-control" id="inputLastname3" placeholder="Please enter your last name">
  </div>
  
  <div class="form-group">
    <input type="email" name="email" class="form-control" id="inputEmail3" placeholder="Please enter your email address">
  </div>
    
  <div class="form-group">
    <input type="userid" name="username" class="form-control" id="inputUserid3" placeholder="Please enter your desired user ID">
  </div>
    
  <div class="form-group">
    <input type="password" name="password" class="form-control" id="inputPassword3" placeholder="Please enter your desired password">
  </div>
    
  <div class="form-group">
    <input type="password" name="password" class="form-control" id="inputPassword3" placeholder="Please re-enter your desired password">
  </div>

  <div class="form-group">
    <button type="submit" class="btn btn-primary center-block">Sign in</button>
  </div>
</form>
  </div>
</div>